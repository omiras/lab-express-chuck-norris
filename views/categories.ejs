<head>
  <link rel="stylesheet" href="https://unpkg.com/@picocss/pico@latest/css/pico.min.css">

</head>

<body>
  <h1>Categories</h1>
  <ul>
    <% categories.forEach(function(cat) { %>
      <a href="/categories?cat=<%= cat %>">
        <li>
          <%= cat %>
        </li>
      </a>
      <% }) %>
  </ul>
</body>